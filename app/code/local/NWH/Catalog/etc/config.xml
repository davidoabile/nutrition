<?xml version="1.0"?>
<config>
    <modules>
        <NWH_Catalog>
            <version>0.1.0</version>
        </NWH_Catalog>
    </modules> 
    <frontend>
        <routers>
            <nwh>
                <use>standard</use>
                <args>
                    <module>NWH_Catalog</module>
                    <frontName>nwh</frontName>
                </args>
            </nwh>
        </routers>  
        <layout>
            <updates>
                <nwh>
                    <file>nwh.xml</file>
                </nwh>
            </updates>
        </layout> 
    </frontend>
    <global>
        <resources>
            <nwh_catalog_setup>
                <setup>
                    <module>NWH_Catalog</module>
                    <class>Mage_Catalog_Model_Resource_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </nwh_catalog_setup>
        </resources>
        
    </global>
</config>